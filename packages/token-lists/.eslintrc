{
  "settings": {
    "import/resolver": {
      "node": {
        "paths": [
          "./src"
        ]
      }
    }
  },
  "env": {
    "es6": true,
    "jest": true
  },
  "extends": "@xliger/eslint-config-ligerswap",
  "rules": {
    "no-console": [
      "warn",
      {
        "allow": [
          "info",
          "warn",
          "error"
        ]
      }
    ]
  }
}